{
  "name": "personality-trait-descriptions",
  "version": "1.1.0",
  "description": "Obtain descriptions for IBM Watson Personality Insights traits",
  "main": "/lib/index.js",
  "scripts": {
    "compile": "browserify -t [ babelify --presets [ es2015 ] ] src/index.js -o lib/index.js",
    "prepublish": "npm run compile",
    "test": "npm run lint && npm run codecov",
    "lint": "eslint .",
    "autofix": "eslint . --fix",
    "codecov": "istanbul cover mocha test && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/personality-insights/trait-descriptions.git"
  },
  "keywords": [
    "personality",
    "insights",
    "trait",
    "description",
    "ibm",
    "watson"
  ],
  "author": "Ary Pablo Batista <arypbatista@gmail.com>",
  "contributor": "April Webster <aprilweb@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/personality-insights/trait-descriptions/issues"
  },
  "homepage": "https://github.com/personality-insights/trait-descriptions#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^3.11.1",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "winston": "^2.2.0"
  },
  "dependencies": {
    "marked": "^0.3.5",
    "remove-markdown": "^0.1.0",
    "underscore": "^1.8.3"
  }
}
